#include <iostream>
#include <string>
using namespace std;

int main()
{
    string name;
    int age;
    char initial;
    double gpa;

    name = "Homer Simpson";
    age = 46;
    initial = 'H';
    gpa = 1.523;

    cout << "Name: " << name << endl;
    cout << "Age: " << age << endl;
    cout << "Initial: " << initial << endl;
    cout << "GPA: " << gpa << endl;
    return 0;
}
